{
    "name": "stoffel/better-console-applications-slidedeck",
    "description": "Slide deck to talk Better Console Applications by chr-hertel",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "stoffel/console-canvas": "@dev",
        "stoffel/console-gradient": "@dev",
        "stoffel/console-headline": "@dev",
        "stoffel/console-image": "@dev",
        "symfony/console": "^5.2@dev"
    },
    "autoload": {
	    "psr-4": {
            "App\\": "src/"
	    }
    },
    "repositories": [
        {"type": "path", "url": "~/Playground/Console/console-canvas"},
        {"type": "path", "url": "~/Playground/Console/console-gradient"},
        {"type": "path", "url": "~/Playground/Console/console-headline"},
        {"type": "path", "url": "~/Playground/Console/console-image"}
    ]
}
